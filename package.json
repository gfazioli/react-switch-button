{
  "name": "react-switch-button",
  "version": "1.1.2",
  "description": "A React UI Component to display an awesome Switch (swipe) Button control",
  "main": "dist/react-switch-button.min.js",
  "scripts": {
    "build": "npm run build:jsx && npm run build:cp && npm run example && npm run example:jsx && npm run less:minify",
    "build:jsx": "babel --presets babel-preset-react src --out-dir dist",
    "less:minify": "lessc -x ./src/react-switch-button.less ./dist/react-switch-button.min.css",
    "build:cp": "uglifyjs ./dist/react-switch-button.js -o ./dist/react-switch-button.min.js",
    "example": "cp ./dist/*min.* ./example/assets/",
    "example:jsx": "babel --presets babel-preset-react ./example/src/ ./example/assets/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gfazioli/react-switch-button.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "javascript",
    "ui",
    "switch",
    "button",
    "swipe"
  ],
  "author": "gfazioli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gfazioli/react-switch-button/issues"
  },
  "homepage": "https://github.com/gfazioli/react-switch-button",
  "peerDependencies": {
    "react": "^15.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-react": "^6.5.0",
    "less": "^2.5.1",
    "react": "^15.3.0",
    "requirejs": "~2.2.0",
    "uglify-js": "^2.4.23"
  }
}
